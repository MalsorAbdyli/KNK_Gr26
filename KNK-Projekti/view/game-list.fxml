<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>



<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="600.0" stylesheets="@../resources/styles/app.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.controllers1.GameListController">
    <children>
        <ScrollPane fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="200.0" VBox.vgrow="ALWAYS">
            <content>
                <FlowPane fx:id="usersPane" hgap="15.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="200.0" prefWrapLength="250.0" vgap="15.0" />
            </content>
        </ScrollPane>
        <HBox fx:id="paginationPane" alignment="CENTER" spacing="15.0" styleClass="pagination-list">
            <children>
                <Button mnemonicParsing="false" text="&lt;" />
                <Button mnemonicParsing="false" text="1" />
                <Button mnemonicParsing="false" text="2" />
                <Button mnemonicParsing="false" text="3" />
                <Button mnemonicParsing="false" text="4" />
                <Label text="..." />
                <Button mnemonicParsing="false" text="14" />
                <Button mnemonicParsing="false" text="&gt;" />
            </children>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
            </padding>
        </HBox>
    </children>
</VBox>
